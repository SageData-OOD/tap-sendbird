{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "is_distinct": {
            "type": "boolean",
            "description": "Indicates whether the channel is distinct."
        },
        "is_public": {
            "type": "boolean",
            "description": "Indicates whether the channel is public."
        },
        "is_super": {
            "type": "boolean",
            "description": "Indicates whether the channel is a supergroup channel."
        },
        "is_ephemeral": {
            "type": "boolean",
            "description": "Indicates whether the channel is ephemeral."
        },
        "is_access_code_required": {
            "type": "boolean",
            "description": "Indicates whether an access code is required to join the channel."
        },
        "freeze": {
            "type": "boolean",
            "description": "Indicates whether the channel is frozen."
        },
        "max_length_message": {
            "type": "integer",
            "description": "The maximum length allowed for messages in this channel."
        },
        "custom_type": {
            "type": "string",
            "description": "A custom type for categorizing the channel."
        },
        "name": {
            "type": "string",
            "description": "The name of the group channel."
        },
        "channel_url": {
            "type": "string",
            "description": "The unique URL identifier for the group channel."
        },
        "created_at": {
            "type": "integer",
            "format": "int64",
            "description": "The timestamp of when the channel was created, in milliseconds since epoch."
        },
        "created_by": {
            "type": "object",
            "description": "Details about the user who created the channel.",
            "properties": {
                "user_id": {
                    "type": "string"
                },
                "nickname": {
                    "type": "string"
                },
                "profile_url": {
                    "type": "string"
                },
                "require_auth_for_profile_image": {
                    "type": "boolean"
                }
            },
            "required": [
                "user_id"
            ]
        },
        "cover_url": {
            "type": [
                "string",
                "null"
            ],
            "description": "The URL of the group's cover image."
        },
        "data": {
            "type": [
                "string",
                "null"
            ],
            "description": "Custom data for the channel, typically in JSON format."
        },
        "member_count": {
            "type": "integer",
            "description": "The total number of members in the group channel."
        },
        "joined_member_count": {
            "type": "integer",
            "description": "The number of members currently joined in the channel."
        },
        "members": {
            "type": "array",
            "description": "Details about the members of the channel.",
            "items": {
                "type": "object",
                "properties": {
                    "user_id": {
                        "type": "string"
                    },
                    "nickname": {
                        "type": "string"
                    },
                    "profile_url": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "is_active": {
                        "type": "boolean"
                    },
                    "is_online": {
                        "type": "boolean"
                    },
                    "last_seen_at": {
                        "type": [
                            "integer",
                            "null"
                        ],
                        "format": "int64"
                    },
                    "state": {
                        "type": "string"
                    },
                    "role": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "Either 'operator' or null."
                    },
                    "metadata": {
                        "type": "object",
                        "additionalProperties": {
                            "type": "string"
                        }
                    }
                },
                "required": [
                    "user_id",
                    "nickname",
                    "state"
                ]
            }
        },
        "delivery_receipt": {
            "type": "object",
            "description": "A mapping of user IDs to their message delivery timestamps.",
            "additionalProperties": {
                "type": "integer",
                "format": "int64"
            }
        },
        "read_receipt": {
            "type": "object",
            "description": "A mapping of user IDs to their message read timestamps.",
            "additionalProperties": {
                "type": "integer",
                "format": "int64"
            }
        },
        "last_message": {
            "type": [
                "object",
                "null"
            ],
            "description": "Details of the most recent message in the channel.",
            "properties": {
                "message_id": {
                    "type": "integer"
                },
                "type": {
                    "type": "string"
                },
                "custom_type": {
                    "type": "string"
                },
                "mention_type": {
                    "type": "string"
                },
                "mentioned_users": {
                    "type": "array",
                    "items": {
                        "type": "object"
                    }
                },
                "created_at": {
                    "type": "integer",
                    "format": "int64"
                },
                "updated_at": {
                    "type": "integer",
                    "format": "int64"
                },
                "is_removed": {
                    "type": "boolean"
                },
                "channel_url": {
                    "type": "string"
                },
                "user": {
                    "type": [
                        "object",
                        "null"
                    ],
                    "properties": {
                        "user_id": {
                            "type": "string"
                        },
                        "nickname": {
                            "type": "string"
                        },
                        "profile_url": {
                            "type": "string"
                        },
                        "metadata": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "required": [
                        "user_id"
                    ]
                },
                "message": {
                    "type": "string"
                },
                "translations": {
                    "type": "object"
                },
                "data": {
                    "type": "string"
                },
                "file": {
                    "type": "object"
                }
            },
            "required": [
                "message_id",
                "type",
                "created_at"
            ]
        },
        "invited_at": {
            "type": "integer",
            "format": "int64",
            "description": "The timestamp of when the invitation was sent."
        },
        "inviter": {
            "type": [
                "object",
                "null"
            ],
            "description": "Details about the user who invited others to the channel.",
            "properties": {
                "user_id": {
                    "type": "string"
                },
                "nickname": {
                    "type": "string"
                },
                "profile_url": {
                    "type": "string"
                },
                "metadata": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "user_id"
            ]
        },
        "unread_message_count": {
            "type": "integer",
            "description": "The number of unread messages in the channel."
        },
        "unread_mention_count": {
            "type": "integer",
            "description": "The number of unread mentions in the channel."
        },
        "metadata": {
            "type": "object",
            "additionalProperties": {
                "type": "string"
            },
            "description": "Custom metadata associated with the group channel."
        }
    },
    "required": [
        "channel_url",
        "created_at",
        "member_count",
        "is_distinct",
        "last_message"
    ],
    "additionalProperties": true
}